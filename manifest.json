{
  "manifest_version": 3,
  "name": "LinkedIn Overlap Calculator",
  "version": "1.0",
  "description": "Calculates overlap or gap between LinkedIn work experiences",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*"],
      "js": ["contentScript.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png"
    }
  }
}
